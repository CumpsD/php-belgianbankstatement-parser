        "name": "robarov/csv2mt940",
        "version": "0.1",
        "source": {
          "url": "https://github.com/robarov/csv2mt940",
          "type": "git",
          "reference": "1cd46d9ece"
        }
      }
    }
  },
  "require": {
    "php": ">=5.3.0",
