  "homepage": "https://github.com/wimverstuyf/php-belgianbankstatement-parser",
  "type": "library",
  "license": "GPL-2.0",
  "authors": [
    { "name": "Wim Verstuyf", "email": "wim.verstuyf@codelicious.be" }
  ],
  "repositories": {
